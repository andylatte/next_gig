%table
  %tr
    %th Angelegte Benutzer
  %tr    
    - @crew_members.each do |user|
      %tr
        %td  
          = user.first_name
        %td
          = user.last_name
        %td
          = user.email
        %td
          = link_to "edit", edit_admin_user_path(user)
        %td
          = link_to "delete", admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' }

= link_to "neuer Nutzer", new_admin_user_path
